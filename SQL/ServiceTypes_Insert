ALTER PROC [dbo].[ServiceTypes_Insert] @Name nvarchar(100)
									   ,@UserId int
									   ,@Id int OUTPUT
as
/* ----- Test Code -----

SELECT *
FROM [dbo].[ServiceTypes]

DECLARE @Name nvarchar(100) = 'Custom Service Type Test'
       ,@UserId int = 30
	   ,@id int

EXECUTE [dbo].[ServiceTypes_Insert] @Name
								   ,@UserId
								   ,@Id OUTPUT

SELECT *
FROM [dbo].[ServiceTypes]
*/

BEGIN
INSERT INTO [dbo].[ServiceTypes] ([Name]
								 ,[UserId])
VALUES (@Name
      ,@UserId)

SET @Id = SCOPE_IDENTITY()

END
